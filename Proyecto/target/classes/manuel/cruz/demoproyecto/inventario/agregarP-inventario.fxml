<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:controller="manuel.cruz.demoproyecto.controllers.inventario.ProductoController"
            xmlns:fx="http://javafx.com/fxml/1">
   <Pane prefHeight="79.0" prefWidth="647.0" style="-fx-background-color: #5A606F;">
      <Text layoutX="151.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="AGREGAR PRODUCTO"
            textAlignment="CENTER">
         <font>
            <Font size="35.0"/>
         </font>
         <fill>
            <LinearGradient>
               <stops>
                  <Stop color="WHITE"/>
                  <Stop color="WHITE" offset="1.0"/>
               </stops>
            </LinearGradient>
         </fill>
      </Text>
   </Pane>
   <Pane layoutY="365.0" prefHeight="35.0" prefWidth="647.0" style="-fx-background-color: #5A606F;"/>
   <Button fx:id="volverButton" onMouseClicked="#onMouseClickedVolverButton" layoutX="258.0" layoutY="243.0"
